	<!-- Units: capacity: MW efficiency: number -->
	<bean id="powerplantABase" class="polep.domain.technology.PowerPlant"
		p:name="Producer A's Base PP" p:nameTechnology="Nuclear" p:capacity="700"
		p:efficiency="0.44">
		<property name="fuel" ref="uranium" />
	</bean>

